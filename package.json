{
  "name": "acai",
  "version": "1.0.0",
  "description": "An AI assistant for developing software.",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "keywords": [
    "cli",
    "ai",
    "llm",
    "coding",
    "assistant"
  ],
  "author": "Travis Ennis",
  "license": "MIT",
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -rf ./dist/",
    "compile": "rm -rf ./dist && tsc && cp -r ./dist/source/. ./dist && rm -rf ./dist/source",
    "lint": "eslint '**/*.ts'",
    "prepare": "npm run compile",
    "test": "c8 mocha dist/test/test-*.js",
    "pretest": "npm run compile",
    "fix": "eslint --fix '**/*.ts'",
    "format": "prettier . --write",
    "dev": "tsx ./source/index.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.1",
    "@inquirer/prompts": "^5.3.8",
    "chalk": "^5.3.0",
    "diff": "^6.0.0-beta",
    "globby": "^14.0.2",
    "handlebars": "^4.7.8",
    "ignore": "^5.3.2",
    "marked": "13.0.0",
    "marked-terminal": "^7.1.0",
    "meow": "^13.2.0",
    "openai": "^4.57.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@eslint/js": "^9.9.1",
    "@types/diff": "^5.2.2",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.5.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  }
}
