{
  "name": "acai",
  "version": "0.0.1",
  "description": "An AI assistant for developing software.",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "bin": {
    "acai": "./dist/index.js"
  },
  "keywords": ["cli", "ai", "llm", "coding", "assistant"],
  "author": "Travis Ennis",
  "license": "MIT",
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -rf ./dist/",
    "compile": "rm -rf ./dist && tsc && cp -r ./dist/source/. ./dist && rm -rf ./dist/source",
    "lint": "biome lint",
    "lint:fix": "biome lint --unsafe --write",
    "lint:staged": "biome lint --error-on-warnings --no-errors-on-unmatched --staged",
    "prepare": "npm run compile",
    "test": "node --import tsx --test test/**/*.test.ts",
    "pretest": "npm run compile",
    "format": "biome format --write",
    "dev": "tsx ./source/index.ts",
    "oxlint": "npx oxlint@latest --ignore-path .gitignore --deny correctness --deny suspicous --deny pedantic --deny style --node-plugin",
    "knip": "npx knip",
    "check": "npx npm-check-updates  --interactive --format group"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.6",
    "@ai-sdk/azure": "^1.0.13",
    "@ai-sdk/openai": "^1.0.11",
    "@inquirer/prompts": "^7.2.1",
    "@mozilla/readability": "^0.5.0",
    "ai": "^4.0.22",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "diff": "^7.0.0",
    "dom-to-semantic-markdown": "^1.2.14",
    "figlet": "^1.8.0",
    "globby": "^14.0.2",
    "handlebars": "^4.7.8",
    "ignore": "^7.0.0",
    "jsdom": "^25.0.1",
    "marked": "13.0.0",
    "marked-terminal": "^7.2.1",
    "meow": "^13.2.0",
    "pdfjs-dist": "^4.9.155",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "pino-roll": "^2.2.0",
    "simple-git": "^3.27.0",
    "ts-pattern": "^5.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/diff": "^6.0.0",
    "@types/figlet": "^1.7.0",
    "@types/jsdom": "^21.1.7",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.10.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
