{
  "name": "acai",
  "version": "1.0.0",
  "description": "An AI assistant for developing software.",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "keywords": ["cli", "ai", "llm", "coding", "assistant"],
  "author": "Travis Ennis",
  "license": "MIT",
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -rf ./dist/",
    "compile": "rm -rf ./dist && tsc && cp -r ./dist/source/. ./dist && rm -rf ./dist/source",
    "lint": "biome lint",
    "lint:fix": "biome lint --unsafe --write",
    "lint:staged": "biome lint --error-on-warnings --no-errors-on-unmatched --staged",
    "prepare": "npm run compile",
    "test": "c8 mocha dist/test/test-*.js",
    "pretest": "npm run compile",
    "format": "biome format --write",
    "dev": "tsx ./source/index.ts",
    "oxlint": "npx oxlint@latest --ignore-path .gitignore --deny correctness --deny suspicous --deny pedantic --deny style --node-plugin"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.51",
    "@ai-sdk/azure": "^0.0.45",
    "@ai-sdk/openai": "^0.0.66",
    "@inquirer/prompts": "^6.0.1",
    "@mozilla/readability": "^0.5.0",
    "ai": "^3.4.9",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "diff": "^7.0.0",
    "dom-to-semantic-markdown": "^1.2.13",
    "figlet": "^1.7.0",
    "globby": "^14.0.2",
    "handlebars": "^4.7.8",
    "ignore": "^6.0.2",
    "jsdom": "^25.0.1",
    "marked": "13.0.0",
    "marked-terminal": "^7.1.0",
    "meow": "^13.2.0",
    "pdfreader": "^3.0.5",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "pino-roll": "^2.1.0",
    "simple-git": "^3.27.0",
    "ts-pattern": "^5.4.0",
    "xdg-basedir": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/diff": "^5.2.2",
    "@types/figlet": "^1.5.8",
    "@types/jsdom": "^21.1.7",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.7.4",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
