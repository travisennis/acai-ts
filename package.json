{
  "name": "@travisennis/acai",
  "version": "0.0.1",
  "description": "An AI assistant for developing software.",
  "type": "module",
  "bin": {
    "acai": "./dist/index.js",
    "acai-lsp": "./dist/lsp.js"
  },
  "keywords": ["cli", "ai", "llm", "coding", "assistant"],
  "author": "Travis Ennis",
  "license": "MIT",
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -rf ./dist/",
    "compile": "tsc --pretty",
    "lint": "biome lint",
    "lint:fix": "biome lint --unsafe --write",
    "lint:staged": "biome lint --error-on-warnings --no-errors-on-unmatched --staged",
    "prepare": "npm run compile",
    "test": "c8 node --no-warnings --test test/**/*.test.ts",
    "pretest": "npm run compile",
    "format": "biome check --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=true --write ./source && biome format --write",
    "dev": "node --no-warnings --env-file=.env ./source/index.ts",
    "oxlint": "npx oxlint@latest --ignore-path .gitignore --deny correctness --deny suspicous --deny pedantic --deny style --node-plugin",
    "knip": "npx knip",
    "check": "npx npm-check-updates  --interactive --format group",
    "cpd": "npx jscpd ./source"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/deepseek": "^0.2.14",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/openai": "^1.3.22",
    "@inquirer/prompts": "^7.5.3",
    "@travisennis/stdlib": "^0.0.12",
    "ai": "^4.3.16",
    "ansi-escapes": "^7.0.0",
    "ansi-regex": "^6.1.0",
    "chalk": "^5.4.1",
    "cheerio": "^1.0.0",
    "cli-highlight": "^2.1.11",
    "cli-table3": "^0.6.5",
    "clipboardy": "^4.0.0",
    "diff": "^8.0.2",
    "exa-js": "^1.8.0",
    "globby": "^14.1.0",
    "ignore": "^7.0.5",
    "log-update": "^6.1.0",
    "marked": "15.0.12",
    "meow": "^13.2.0",
    "node-emoji": "^2.2.0",
    "node-notifier": "^10.0.1",
    "ora": "^8.2.0",
    "p-throttle": "^7.0.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "pino-roll": "^3.1.0",
    "simple-git": "^3.27.0",
    "supports-hyperlinks": "^4.1.0",
    "terminal-link": "^4.0.0",
    "tiktoken": "^1.0.21",
    "tree-sitter": "0.21.1",
    "tree-sitter-java": "0.23.5",
    "tree-sitter-javascript": "0.23.1",
    "tree-sitter-python": "0.23.4",
    "tree-sitter-typescript": "0.23.2",
    "ts-pattern": "^5.7.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12",
    "wrap-ansi": "^9.0.0",
    "zod": "^3.25.49"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^1.1.3",
    "@biomejs/biome": "1.9.4",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.15.29",
    "@types/node-notifier": "^8.0.5",
    "c8": "^10.1.3",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
