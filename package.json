{
  "name": "acai",
  "version": "1.0.0",
  "description": "An AI assistant for developing software.",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "keywords": ["cli", "ai", "llm", "coding", "assistant"],
  "author": "Travis Ennis",
  "license": "MIT",
  "scripts": {
    "build": "npm run compile",
    "clean": "rm -rf ./dist/",
    "compile": "rm -rf ./dist && tsc && cp -r ./dist/source/. ./dist && rm -rf ./dist/source",
    "lint": "biome lint",
    "prepare": "npm run compile",
    "test": "c8 mocha dist/test/test-*.js",
    "pretest": "npm run compile",
    "fix": "biome lint --write",
    "format": "biome format --write",
    "dev": "tsx ./source/index.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.50",
    "@ai-sdk/openai": "^0.0.60",
    "@anthropic-ai/sdk": "^0.27.3",
    "@inquirer/prompts": "^5.5.0",
    "ai": "^3.3.37",
    "chalk": "^5.3.0",
    "diff": "^7.0.0",
    "globby": "^14.0.2",
    "handlebars": "^4.7.8",
    "ignore": "^5.3.2",
    "marked": "13.0.0",
    "marked-terminal": "^7.1.0",
    "meow": "^13.2.0",
    "openai": "^4.61.0",
    "simple-git": "^3.26.0",
    "xdg-basedir": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.0",
    "@types/diff": "^5.2.2",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.5.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
